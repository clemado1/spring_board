<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.javalec.spring_board.dao.BDao">
	<select id="listDao" resultType="com.javalec.spring_board.dto.BDto">
		SELECT BID, BNAME, BTITLE, BDATE, BHIT, BGROUP, BSTEP, BINDENT FROM BOARD ORDER BY BGROUP DESC, BSTEP ASC
	</select>
	
	
	<insert id="writeDao">
	</insert>
	
	
	<delete id="deleteDao">
	</delete>
	
	
</mapper>
